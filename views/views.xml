        <record id="menu_university_root" model="ir.ui.menu">
            <field name="name">University</field>
            <field name="action" ref="action_university_main"/>
            <field name="sequence" type="int">10</field>
        </record>
        <record id="action_university_main" model="ir.actions.act_window">
